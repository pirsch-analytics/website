{
    "dev": true,
    "server": {
        "host": "localhost.com",
        "port": 8080,
        "shutdown_time": 30,
        "write_timeout": 5,
        "read_timeout": 5,
        "hostname": "localhost.com",
        "tls_cert_file": "test/certs/cert.pem",
        "tls_key_file": "test/certs/key.pem",
        "secure_cookies": true,
        "cookie_domain_name": "localhost.com"
    },
    "content": {
        "provider": "git",
        "update_seconds": 30,
        "repository": "https://github.com/pirsch-analytics/website.git"
    },
    "cors": {
        "origins": "*",
        "loglevel": "info"
    },
    "sass": {
        "entrypoint": "main.scss",
        "dir": "assets/scss",
        "watch": true,
        "out": "static/css/main.css",
        "out_source_map": "static/css/main.css.map"
    },
    "js": {
        "entrypoint": "main.js",
        "dir": "assets/js",
        "watch": true,
        "out": "static/js/main.js"
    }
}
